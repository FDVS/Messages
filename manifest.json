{
  "manifest_version": 2,
  "name": "Messages",
  "author": "Jeremy Chui",
  "version": "1.0.0",
  "description": "Combining popular instant messaging networks into one Chrome application.",
  "permissions": ["http://fonts.googleapis.com/*","webview","browser"],
  "icons": { "16": "assets/icon16.png",
           "48": "assets/icon48.png",
           "128": "assets/icon128.png",
          "256": "assets/icon256.png" },
  "app": {
    "background": {
      "scripts": ["jquery.min.js","main.js"]
    }
  }
}